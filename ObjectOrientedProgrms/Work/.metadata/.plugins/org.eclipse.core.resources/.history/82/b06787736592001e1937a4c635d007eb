public class DanceRunner{
	public static void main(String args[]){
		Dance.dance("Classic");
		System.out.println(Dance.dance("drametic", "action"));
		Dance.dance(12, 3, 5, "naturally");
		Dance.dance(5, 3, 7);
		Dance.dance("create", "expression", "art", "helps to health", "any body can dance", "hip-hop", "music");
	}
}