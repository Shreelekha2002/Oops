public class CricketRunner{
	public static void main(String args[]){
		String names[]={"rahul", "kohli"};
		Cricket.playerNames(names);
		String name1[]={"India-Pakistan", "india-srilanka", "pakistan-australia"};
		Cricket.matchNames(name1);
		String groundNameList[]={"Banglore", "Delhi", "Hyderabadh", "pune", "madras"};
		Cricket.groundNames(groundNameList);
		int daysList[]={12, 13, 14, 15};
		Cricket.matchDays(daysList);
	}
}